{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    }
  ],
  "redirects": [
    {
      "source": "/indexRMFileConverter.html",
      "destination": "/products",
      "permanent": true
    },
    {
      "source": "/indexRMSecure(.*).html",
      "destination": "/products",
      "permanent": true
    },
    {
      "source": "/indexRMText(.*).html",
      "destination": "/products",
      "permanent": true
    },
    {
      "source": "/index\\(Products\\).html",
      "destination": "/products",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ]
}
